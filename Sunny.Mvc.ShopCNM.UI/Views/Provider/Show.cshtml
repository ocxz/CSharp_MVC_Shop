@model Sunny.Mvc.ShopCNM.UI.Models.Show.ProviderShow
@using Sunny.Mvc.ShopCNM.Common

<div class="location">
    <strong>你现在所在的位置是:</strong>
    <span>供应商管理页面 >> 信息查看</span>
</div>
<div class="providerView">
    <p><strong>供应商编码：</strong><span>@Model.Pid</span></p>
    <p><strong>供应商名称：</strong><span>@Model.PName</span></p>
    <p><strong>创建时间：</strong><span>@Model.CreateTime</span></p>
    <p><strong>联系人：</strong><span>@Model.LinkMan</span></p>
    <p><strong>联系电话：</strong><span>@Model.LinkPhone</span></p>
    <p><strong>传真：</strong><span>@Model.Fax</span></p>
    <p><strong>地址：</strong><span>@Model.PAddress</span></p>
    <p><strong>描述：</strong><span>@Model.Detail</span></p>

    <a href="@MyUtils.GetUrl("../Provider",new { pNamelike = ViewData["pNamelike"], pageIndex = Session["pageIndex"] })">返回</a>
</div>

